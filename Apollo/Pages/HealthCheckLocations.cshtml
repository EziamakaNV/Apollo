@page
@model Apollo.Pages.HealthCheckLocationsModel
@{
    ViewData["Title"] = "Free Health Check Locations";
}

<h2>@ViewData["Title"]</h2>

<p>Below are some locations where you can get free blood pressure, heart rate, temperature, and height checks in the U.S.:</p>

<ul class="list-group">
    <li class="list-group-item">Local Health Department, [City, State]</li>
    <li class="list-group-item">Community Health Center, [City, State]</li>
    <li class="list-group-item">Free Clinic, [City, State]</li>
    <li class="list-group-item">Mobile Health Unit, [City, State]</li>
</ul>
